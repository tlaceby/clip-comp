{
  "name": "clipz",
  "version": "1.1.5",
  "description": "GUI For easier and faster compression of video clips. Uses FFMPEG under the hood.",
  "main": "./compiled/index.js",
  "scripts": {
    "build": "tsc",
    "fresh-start": "npm install && npm run start",
    "pack": "npm install && npm run make",
    "make": "npm run build && electron-builder",
    "start": "tsc && electron .",
    "deploy": "npm run pack && tsc && electron-builder build --win --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylerbadger23/clip-comp.git"
  },
  "build": {
    "appId": "clipz",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "logo-transparent.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "logo-transparent.ico",
      "uninstallerIcon": "logo-transparent.ico",
      "uninstallDisplayName": "clipz",
      "license": "readme.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "keywords": [],
  "author": "Tyler Laceby",
  "license": "ISC",
  "devDependencies": {
    "@types/electron-json-storage": "^4.5.0",
    "@types/ffprobe": "^1.1.2",
    "@types/node": "^16.7.13",
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "electron-json-storage": "^4.5.0",
    "electron-updater": "^4.3.9",
    "ffprobe": "^1.1.2",
    "file-type": "^16.5.3",
    "read-chunk": "^4.0.2"
  }
}
